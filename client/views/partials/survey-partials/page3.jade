	.form-group
		label.control-label(for="survey.end") end date
		.col-sm-10
			p.input-group
				input.form-control(type="text", datepicker-popup="{{format}}", ng-model="dt", is-open="$parent.opened", min-date="minDate", max-date="'2015-06-22'", datepicker-options="dateOptions", date-disabled="disabled(date, mode)", ng-required="true", close-text="Close")
				span.input-group-btn
					button.btn.btn-default(type="button", ng-click="open($event)")
						i.glyphicon.glyphicon-calendar

	.form-group
		.controls
			button.btn(type="submit") Done!
